@{
}

@model List<Suckhoe>

<div class="row">
    @foreach (var item in Model)
    {
        <div>
            <div class="thumbnail">
                <h3>@item.Category.CategoryName</h3>
                <img class="img-responsive fit-image" src="@item.ImageUrl" alt="Alternate Text" width="50%" />
                <div class="caption">
                    <h3>@item.Name</h3>
                    <p>@item.Description</p>
                </div>
            </div>
        </div>
    }
</div>